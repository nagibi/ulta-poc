            .subscribe((mensagens) => {
            this.mensagens = mensagens;
            this.carregarComboStatus();
            this.carregarComboTipoDocumento();
            this.criarForm();
            this.criarSubheader();
            this.carregarPagina();
        });
    }
    filtroAvancado() {
        //data criacao
        var isDataCriacaoValid = this.utilService.validarDataInicialFinal(this.form, 'dataCriacaoInicial', 'dataCriacaoFinal');
        //data atualizacao
        var isDataAtualizacaoValid = this.utilService.validarDataInicialFinal(this.form, 'dataAtualizacaoInicial', 'dataAtualizacaoFinal');
        if (isDataCriacaoValid && isDataAtualizacaoValid)
            this.filtrarGrid(this.gridService);
        else if (!isDataCriacaoValid)
            this.messageService.create('MSG000186');
        else
            this.messageService.create('MSG000187');
    }
    criarSubheader() {
        //cadastrar
        this.cadastrarBtn = {
            text: this.mensagens['MSG000154.descricao'],
            type: src_app_core_models_button_model__WEBPACK_IMPORTED_MODULE_1__["ButtonType"].LIGHT_SUCCESS,
            click: () => {
                this.router.navigate([`${DocumentoPesquisarComponent.ROTA}/novo`]);
