            'MSG000118.descricao',
            'MSG000132.descricao',
            'MSG000133.descricao',
            'MSG000135.descricao',
            'MSG000136.descricao',
            'MSG000137.descricao',
            'MSG000138.descricao',
            'MSG000225.descricao',
            'MSG000141.descricao',
            'MSG000142.descricao',
            'MSG000149.descricao',
            'MSG000150.descricao',
            'MSG000155.descricao',
            'MSG000183.descricao',
            'MSG000007.descricao',
            'MSG000220.descricao',
        ])
            .subscribe((mensagens) => {
            this.mensagens = mensagens;
            this.carregarComboUfs();
            this.criarForm();
            this.criarSubheader();
            this.carregarPagina();
        });
    }
    carregarComboMunicipios() {
        this.cidadeList = [];
        var ufId = this.form.controls['ufId'].value;
        this.localidadeService.obterMunicipios(ufId).subscribe(resp => {
            resp.result.sort(this.utilService.dynamicSort('nome')).forEach(data => {
                this.cidadeList.push({ id: data.id, text: data.nome });
            });
        }, erro => {
            this.setError(erro);
        });
    }
    carregarComboUfs() {
        this.localidadeService.obterEstados().subscribe(resp => {
            resp.result.sort(this.utilService.dynamicSort('nome')).forEach(data => {
                this.ufList.push({ id: data.id, text: data.nome });
            });
        }, erro => {
            this.setError(erro);
        });
    }
    criarForm() {
        this.form = this.fb.group({
            nome: [
                '',
                _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].compose([_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].maxLength(100)]),
            ],
            municipioId: [
                '',
                _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].compose([_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]),
            ],
            ufId: [
                '',
                _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].compose([_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]),
            ],
            dataCriacao: [''],
            dataAtualizacao: [''],
            usuarioCriacaoId: [''],
            usuarioAtualizacaoId: [''],
        });
    }
    carregarPagina() {
        this.activeRoute.params.subscribe((params) => {
            this.id = +params['id'];
            this.caregarRegistro();
        });
    }
    criarSubheader() {
        //salvar
        this.btnSalvar = {
            text: this.mensagens['MSG000092.descricao'],
            type: src_app_core_models_button_model__WEBPACK_IMPORTED_MODULE_2__["ButtonType"].LIGHT_PRIMARY,
            click: () => this.salvar(false),
            icon: 'flaticon2-writing',
        };
        //salvar & novo
        var btnSalvarNovo = {
            text: this.mensagens['MSG000133.descricao'],
            type: src_app_core_models_button_model__WEBPACK_IMPORTED_MODULE_2__["ButtonType"].LIGHT_PRIMARY,
            click: () => this.salvar(true),
            icon: 'flaticon2-writing',
        };
        //salvar & continuar
        var btnSalvarContinuar = {
            text: this.mensagens['MSG000132.descricao'],
            type: src_app_core_models_button_model__WEBPACK_IMPORTED_MODULE_2__["ButtonType"].LIGHT_PRIMARY,
            click: () => this.salvar(false),
            icon: 'flaticon2-medical-records',
        };
        this.btnSalvar.actions = [btnSalvarContinuar, btnSalvarNovo];
        //excluir
        this.btnExcluir = {
            text: this.mensagens['MSG000138.descricao'],
            type: src_app_core_models_button_model__WEBPACK_IMPORTED_MODULE_2__["ButtonType"].LIGHT_DANGER,
            click: () => this.deletar(),
            icon: 'fa fa-trash-alt icon-sm',
            visible: false,
        };
        //inativar
        this.btnInativar = {
            text: this.mensagens['MSG000137.descricao'],
            type: src_app_core_models_button_model__WEBPACK_IMPORTED_MODULE_2__["ButtonType"].LIGHT_DANGER,
            click: () => this.ativar(),
            icon: 'fa fa-thumbs-down icon-sm',
            visible: false,
        };
        //ativar
        this.btnAtivar = {
            text: this.mensagens['MSG000136.descricao'],
            type: src_app_core_models_button_model__WEBPACK_IMPORTED_MODULE_2__["ButtonType"].LIGHT_SUCCESS,
            click: () => this.ativar(),
            icon: 'fa fa-thumbs-up icon-sm',
            visible: false,
        };
        //voltar
        var btnVoltar = {
            text: this.mensagens['MSG000155.descricao'],
            type: src_app_core_models_button_model__WEBPACK_IMPORTED_MODULE_2__["ButtonType"].DEFAULT,
            click: () => {
                this.router.navigate([`${src_app_pages_admin_components_bairro_bairro_pesquisar_bairro_pesquisar_component__WEBPACK_IMPORTED_MODULE_5__["BairroPesquisarComponent"].ROTA}`]);
            },
            icon: 'fa fa-reply-all icon-sm',
        };
        var buttons = [
            btnVoltar,
            this.btnExcluir,
            this.btnAtivar,
            this.btnInativar,
            this.btnSalvar,
        ];
        this.subheader.buttons = buttons;
        this.subheader.label = this.label;
        this.subheaderService.create(this.subheader);
    }
    atualizarStatus() {
        this.label.html = this.utilService.createLabelStatus(this.registroAtual.status);
        if (this.registroAtual.status == 1) {
            this.btnInativar.visible = true;
            this.btnAtivar.visible = false;
        }
        else {
            this.btnInativar.visible = false;
            this.btnAtivar.visible = true;
        }
    }
    caregarRegistro() {
        if (this.id) {
            this.bairroService.obterBairroId(this.id).subscribe((resp) => {
                this.registroAtual = resp.result;
                this.atualizarStatus();
                this.btnExcluir.visible = true;
                this.subheaderService.title = this.mensagens['MSG000225.descricao'];
                this.subheaderService.text = `${this.registroAtual.id} - ${this.registroAtual.nome}`;
                this.form.patchValue(resp.result);
                this.carregarComboMunicipios();
            }, (erro) => {
                this.setError(erro);
            }, () => { });
        }
        else {
            this.label.visible = false;
            this.subheaderService.title = this.mensagens['MSG000118.descricao'];
            this.subheaderService.text = this.mensagens['MSG000135.descricao'];
        }
    }
    ativar() {
        var template = this.registroAtual;
        template.status = this.registroAtual.status == 0 ? 1 : 0;
        this.bairroService.statusBairro(this.id, template.status).subscribe((resp) => {
            this.messageService.create(resp.message);
            this.registroAtual = resp.result;
            this.atualizarStatus();
        }, (erro) => {
            this.setError(erro);
        });
    }
    salvar(isNovo = false) {
        if (this.validar()) {
            const dadosForm = this.form.value;
            let model = new _models_bairro_model__WEBPACK_IMPORTED_MODULE_6__["Bairro"]();
            if (this.id > 0) {
                model.id = this.id;
            }
            model.nome = dadosForm.nome;
            model.municipioId = dadosForm.municipioId;
            model.ufId = dadosForm.ufId;
            this.bairroService.salvarBairro(model).subscribe((resp) => {
                this.messageService.create(resp.message);
                if (resp.message === 'MSG000111' || resp.message === 'MSG000151') {
                    if (isNovo) {
                        this.cancelar();
                        this.router.navigate([`${src_app_pages_admin_components_bairro_bairro_pesquisar_bairro_pesquisar_component__WEBPACK_IMPORTED_MODULE_5__["BairroPesquisarComponent"].ROTA}/novo`]);
                    }
                    else if (this.id != resp.result.id) {
                        this.router.navigate([
                            `${src_app_pages_admin_components_bairro_bairro_pesquisar_bairro_pesquisar_component__WEBPACK_IMPORTED_MODULE_5__["BairroPesquisarComponent"].ROTA}/${resp.result.id}`,
                        ]);
                    }
                    else {
                        this.ngOnInit();
                    }
                }
            }, (erro) => {
                this.setError(erro);
            });
        }
    }
    deletar() {
        const modalRef = this.modalService.open(src_app_core_components_modal_modal_component__WEBPACK_IMPORTED_MODULE_4__["ModalComponent"], this.modalOptions);
        modalRef.componentInstance.modal = {
            title: this.mensagens['MSG000142.descricao'],
            conteudo: this.mensagens['MSG000141.descricao'],
        };
        modalRef.result.then(() => {
            this.bairroService.deletarBairro(this.id).subscribe((resp) => {
                this.messageService.create(resp.message);
                if (resp.message === 'MSG000144') {
                    this.router.navigate([`${src_app_pages_admin_components_bairro_bairro_pesquisar_bairro_pesquisar_component__WEBPACK_IMPORTED_MODULE_5__["BairroPesquisarComponent"].ROTA}/novo`]);
                }
            }, (erro) => {
                this.setError(erro);
            });
            this.messageService.create('MSG000144');
        }, () => { });
    }
    cancelar() {
        this.form.reset();
    }
}
