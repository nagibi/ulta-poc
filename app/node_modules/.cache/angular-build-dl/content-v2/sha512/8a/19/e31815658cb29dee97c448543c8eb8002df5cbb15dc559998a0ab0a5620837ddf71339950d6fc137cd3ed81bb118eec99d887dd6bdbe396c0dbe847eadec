class TemplateEmailModalComponent extends src_app_core_components_form_base_form_base_component__WEBPACK_IMPORTED_MODULE_1__["FormBaseComponent"] {
    constructor(fb, templateEmailService, translateService, authService, router, activeRoute, messageService, modalService, activeModal) {
        super(fb, router, translateService, messageService, modalService);
        this.fb = fb;
        this.templateEmailService = templateEmailService;
        this.translateService = translateService;
        this.authService = authService;
        this.router = router;
        this.activeRoute = activeRoute;
        this.messageService = messageService;
        this.modalService = modalService;
        this.activeModal = activeModal;
        this.passEntry = new _angular_core__WEBPACK_IMPORTED_MODULE_0__["EventEmitter"]();
    }
    ngOnInit() {
        this.msgsError = [];
        this.criarForm();
    }
    validar() {
        var controls = this.form.controls;
        if (this.form.invalid) {
            Object.keys(controls).forEach((controlName) => {
                controls[controlName].markAsTouched();
            });
            this.messageService.create('MSG000034');
            return false;
        }
        else {
            return true;
        }
    }
    criarForm() {
        this.form = this.fb.group({
            email: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].compose([_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].email])],
        });
    }
    submit() {
        if (this.validar()) {
            const dadosForm = this.form.value;
            this.templateEmailService
                .enviarEmail(this.modal.id, dadosForm.email)
                .subscribe((resp) => {
                this.messageService.create(resp.message);
                if (resp.message == 'MSG000148') {
                    this.activeModal.close();
                }
            });
        }
    }
