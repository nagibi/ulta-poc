        gridOption.apiServiceHttp = `${src_environments_environment__WEBPACK_IMPORTED_MODULE_2__["environment"].apiUrl}/${_services_template_email_service__WEBPACK_IMPORTED_MODULE_3__["TemplateEmailService"].API_TEMPLATE_EMAIL}${window.location.search}`;
