class DocumentoPesquisarComponent extends src_app_core_components_form_base_form_base_component__WEBPACK_IMPORTED_MODULE_3__["FormBaseComponent"] {
    constructor(toastService, fb, translateService, authService, router, route, activeRoute, messageService, modalService, documentoService, utilService, subheaderService, gridService) {
        super(fb, router, translateService, messageService, modalService, utilService);
        this.toastService = toastService;
        this.fb = fb;
        this.translateService = translateService;
        this.authService = authService;
        this.router = router;
        this.route = route;
        this.activeRoute = activeRoute;
        this.messageService = messageService;
        this.modalService = modalService;
        this.documentoService = documentoService;
        this.utilService = utilService;
        this.subheaderService = subheaderService;
        this.gridService = gridService;
        this.statusList = [];
        this.tipoList = [];
    }
    carregarComboStatus() {
        this.documentoService.obterStatusDocumento().subscribe(resp => {
            resp.result.forEach(data => {
                this.statusList.push({ id: data.valor, text: data.descricao });
            });
        }, erro => {
            this.setError(erro);
        });
    }
    carregarComboTipoDocumento() {
        this.documentoService.obterTipoArquivo().subscribe(resp => {
            resp.result.forEach(data => {
                this.tipoList.push({ id: data.valor, text: data.descricao });
            });
        }, erro => {
            this.setError(erro);
        });
    }
    criarForm() {
        this.form = this.fb.group({
            id: [''],
            nome: [''],
            nomeOriginal: [''],
            tipo: [''],
            caminho: [''],
            extensao: [''],
            status: [''],
            dataCriacaoInicial: [''],
            dataCriacaoFinal: [''],
            dataAtualizacaoInicial: [''],
            dataAtualizacaoFinal: [''],
        });
    }
    ngOnInit() {
        this.modalOptions = {
            backdrop: 'static',
            backdropClass: 'customBackdrop',
        };
        this.translateService
            .get([
            'MSG000017.descricao',
            'MSG000142.descricao',
