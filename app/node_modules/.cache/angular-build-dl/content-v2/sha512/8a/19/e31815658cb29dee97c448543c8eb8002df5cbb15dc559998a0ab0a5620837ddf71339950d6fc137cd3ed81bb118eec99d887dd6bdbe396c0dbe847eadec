        //     this.router.navigate([
        //       `${DocumentoPesquisarComponent.ROTA}/${obj.id}`,
        //     ]),
        // };
        this.reProcessarGridBtn = {
            icon: 'fa fa-sync icon-nm',
            type: src_app_core_models_button_model__WEBPACK_IMPORTED_MODULE_1__["ButtonType"].LIGHT_PRIMARY,
            title: this.mensagens['MSG000267.descricao'],
            visible: true,
            permission: ['documento.cadastrar', 'documento.atualizar'],
            click: (obj) => this.reProcessar([obj]),
        };
        var gridOption = new Object();
        gridOption.apiServiceHttp = `${src_environments_environment__WEBPACK_IMPORTED_MODULE_2__["environment"].apiUrl}/${_services_documento_service__WEBPACK_IMPORTED_MODULE_6__["DocumentoService"].API_DOCUMENTO}${window.location.search}`;
        // gridOption.routeBase = 'admin/documento';
        gridOption.orderBy = 'id';
        gridOption.orderType = 'desc';
        gridOption.columns = [
            {
                field: 'id',
                fakeField: 'id',
                label: this.mensagens['MSG000156.descricao'],
                sortable: true,
            },
            // {
            //   field: 'id',
            //   fakeField: 'thumbGrid',
            //   label: this.mensagens['MSG000257.descricao'],
            //   // sortable: true,
            // },
            {
                field: 'nome',
                label: this.mensagens['MSG000017.descricao'],
                sortable: true,
            },
            {
                field: 'nomeOriginal',
                label: this.mensagens['MSG000234.descricao'],
                sortable: true,
            },
            // {
            //   field: 'tipo',
            //   fakeField: 'tipoDocumentoGrid',
            //   label: this.mensagens['MSG000236.descricao'],
            //   sortable: true,
            // },
            {
                field: 'caminho',
                label: this.mensagens['MSG000235.descricao'],
                sortable: true,
            },
            {
                field: 'extensao',
                label: this.mensagens['MSG000237.descricao'],
                sortable: true,
            },
            {
                field: 'status',
                fakeField: 'statusGrid',
                label: this.mensagens['MSG000159.descricao'],
                align: 'center',
                sortable: true,
            },
            {
                field: 'dataAtualizacao',
                label: this.mensagens['MSG000160.descricao'],
                align: 'center',
                sortable: true,
            },
            {
                field: 'dataCriacao',
                label: this.mensagens['MSG000161.descricao'],
                align: 'center',
                sortable: true,
            },
            {
                label: this.mensagens['MSG000162.descricao'],
                align: 'center',
                actions: [
                    this.reProcessarGridBtn
                ],
            },
        ];
        // gridOption.onError (err=>{
        //   var teste = err;
        // });
        gridOption.onSelectedItem = (itens) => {
            this.btnProcessar.visible = itens.length > 0;
            this.btnAtivar.visible = this.btnInativar.visible = false;
            if (itens.length > 0) {
                var primeiroItem = itens[0];
                var isIguais = itens.every((item) => item.status == primeiroItem.status);
                if (isIguais) {
                    if (primeiroItem.status == 1) {
                        this.btnAtivar.visible = false;
                        this.btnInativar.visible = true;
                    }
                    else {
                        this.btnAtivar.visible = true;
                        this.btnInativar.visible = false;
                    }
                }
            }
        };
        gridOption.onBenforeData = (data) => {
            var dados = [];
            data.forEach(entity => {
                var dado = entity;
                dado.statusGrid = this.utilService.createLabelStatusDocumento(entity.status);
                // dado.tipoDocumentoGrid = this.utilService.createLabelTipoDocumento(entity.status);
                // dado.thumbGrid = `
                // <div class="symbol symbol-50 symbol-light mr-4">
                //                                 <a class="symbol-label" href="${entity.caminho}" title="${this.mensagens['MSG000258']}" target="_blank"><img  src="${entity.caminho}" class="h-75 align-self-end" alt=""></a>
                // 															</div>`;
                dados.push(dado);
            });
            return dados;
        };
        this.grid = this.gridService;
        this.grid.create(gridOption);
        this.grid.search();
    }
    resetGrid() {
        this.btnAtivar.visible = false;
        this.btnInativar.visible = false;
        this.btnProcessar.visible = false;
        this.gridService.search();
    }
    ativar(itens) {
        var status = itens[0].status == 0 ? 1 : 0;
        var requisisoes = [];
        itens.forEach((item) => {
            requisisoes.push(this.documentoService.statusDocumento(item.id, status).pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_7__["catchError"])(erro => Object(rxjs__WEBPACK_IMPORTED_MODULE_5__["of"])(erro))));
        });
        Object(rxjs__WEBPACK_IMPORTED_MODULE_5__["forkJoin"])(requisisoes).subscribe((x) => {
            var resp = x[0];
            this.messageService.create(resp.message);
            this.resetGrid();
        });
    }
    cancelar() {
        this.form.reset();
        this.filtrarGrid(this.gridService);
    }
    reProcessar(itens) {
        const modalRef = this.modalService.open(src_app_core_components_modal_modal_component__WEBPACK_IMPORTED_MODULE_4__["ModalComponent"], this.modalOptions);
        modalRef.componentInstance.modal = {
            title: this.mensagens['MSG000142.descricao'],
            conteudo: this.mensagens['MSG000268.descricao'],
        };
        modalRef.result.then(() => {
            var registros = [];
            itens.forEach((item) => {
                registros.push(this.documentoService.deletarDocumento(item.id).pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_7__["catchError"])(erro => Object(rxjs__WEBPACK_IMPORTED_MODULE_5__["of"])(erro))));
            });
            Object(rxjs__WEBPACK_IMPORTED_MODULE_5__["forkJoin"])(registros).subscribe((resp) => {
                var indexError = resp.map(item => { return item.status.toLowerCase(); }).indexOf('erro');
                if (indexError > -1) {
                    this.messageService.create(resp[indexError].message);
                }
                else {
                    this.messageService.create(resp[0].message);
                }
                this.resetGrid();
            });
        }, () => { });
    }
}
