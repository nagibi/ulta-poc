der = true;\n\n      this.translateService.get(this.text).subscribe((message) => {\n        this.text = message;\n      });\n    }\n    if (this.subtitle != null) {\n      this.translateService.get(this.subtitle).subscribe((message) => {\n        this.subtitle = message;\n      });\n    }\n  }\n}\n","<!-- <div class=\"card card-custom\" data-card=\"true\" id=\"kt_card_3\"> -->\n  <!-- <div class=\"card-header\" #ref> -->\n    <!-- <ng-content select=\"[card-header]\"></ng-content> -->\n    <!-- <div class=\"card-title\">\n      <span class=\"card-icon\">\n        <i class=\"flaticon2-chat-1 text-primary\"></i>\n      </span>\n      <h3 class=\"card-label\">Card Footer\n        <small>sub title</small></h3>\n    </div>\n    <div class=\"card-toolbar\">\n      <a href=\"#\" class=\"btn btn-icon btn-circle btn-sm btn-light-primary mr-1\" data-card-tool=\"toggle\">\n        <i class=\"ki ki-arrow-down icon-nm\"></i>\n      </a>\n      <a href=\"#\" class=\"btn btn-icon btn-circle btn-sm btn-light-success mr-1\" data-card-tool=\"reload\">\n        <i class=\"ki ki-reload icon-nm\"></i>\n      </a>\n      <a href=\"#\" class=\"btn btn-icon btn-circle btn-sm btn-light-danger\" data-card-tool=\"remove\">\n        <i class=\"ki ki-close icon-nm\"></i>\n      </a>\n    </div> -->\n  <!-- </div> -->\n  <!-- <div class=\"card-body\"> -->\n    <!-- <ng-content select=\"[card-body]\"></ng-content> -->\n  <!-- </div> -->\n  <!-- <div class=\"card-footer d-flex justify-content-between\"> -->\n    <!-- <ng-content select=\"[card-footer]\"></ng-content> -->\n    <!-- <a href=\"#\" class=\"btn btn-light-primary font-weight-bold\">Manage</a>\n    <a href=\"#\" class=\"btn btn-outline-secondary font-weight-bold\">Learn more</a> -->\n  <!-- </div> -->\n<!-- </div> -->\n\n<div class=\"card card-custom\" [ngClass]=\"bg? 'text-white ' + bg : ''\">\n  <div *ngIf=isHeader class=\"card-header\" [attr.title]=\"isCollapsed ? this.openText : this.closeText\" style=\"cursor: pointer;\" (click)=\"i