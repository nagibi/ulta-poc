 {\r\n      this.grupoService.obterTotalGrupos().subscribe(resp => {\r\n        this.totalGrupos = resp.result;\r\n      });\r\n    }\r\n\r\n    if (this.authService.usuarioTemPermissao(['usuario-obter'])) {\r\n      this.documentoService.obterTotalDocumentos().subscribe(resp => {\r\n        this.totalDocumentos = resp.result;\r\n      });\r\n    }\r\n  }\r\n\r\n\r\n}\r\n","\r\n<div class=\"row\">\r\n    <div class=\"col-xl-3\" [permissao]=\"['usuario-obter']\">\r\n        <!--begin::Stats Widget 29-->\r\n        <div class=\"card card-custom bgi-no-repeat card-stretch gutter-b\" [attr.title]=\"'MSG000200.descricao' | translate\">\r\n            <!--begin::Body-->\r\n            <div class=\"card-body\">\r\n                <span class=\"svg-icon svg-icon-primary svg-icon-2x\"><!--begin::Svg Icon | path:C:\\wamp64\\www\\keenthemes\\themes\\metronic\\theme\\html\\demo1\\dist/../src/media/svg/icons\\Communication\\Add-user.svg--><svg xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" width=\"24px\" height=\"24px\" viewBox=\"0 0 24 24\" version=\"1.1\">\r\n                    <g stroke=\"none\" stroke-width=\"1\" fill=\"none\" fill-rule=\"evenodd\">\r\n                        <polygon points=\"0 0 24 0 24 24 0 24\"/>\r\n                        <path d=\"M18,8 L16,8 C15.4477153,8 15,7.55228475 15,7 C15,6.44771525 15.4477153,6 16,6 L18,6 L18,4 C18,3.44771525 18.4477153,3 19,3 C19.5522847,3 20,3.44771525 20,4 L20,6 L22,6 C22.5522847,6 23,6.44771525 23,7 C23,7.55228475 22.5522847,8 22,8 L20,8 L20,10 C20,10.5522847 19.5522847,11 19,11 C18.4477153,11 18,10.5522847 18,10 L18,8 Z M9,11 C6.790861,11 5,9.209139 5,7 C5,4.790861 6.790861,3 9,3 C11.209139,3 13,4.790861 13,7 C13,9.209139 11.209139,11 9,11 Z\" fill=\"#000000\" fill-rule=\"nonzero\" opacity=\"0.3\"/>\r\n                        <path d=\"M0.00065168429,20.1992055 C0.388258525,15.4265159 4.26191235,13 8.98334134,13 C13.7712164,13 17.7048837,15.2931929 17.9979143,20.2 C18.0095879,20.3954741 